.selectedSection {
    margin-top: 2px;
    margin-right: 2px;
    margin-bottom: 2px;
    margin-left: 2px;
    padding-top: 0;
    padding-right: 0;
    padding-bottom: 0;
    padding-left: 0;
    flex-wrap: nowrap;
    border-top-width: 2.2px;
    border-right-width: 2.2px;
    border-bottom-width: 2.2px;
    border-left-width: 2.2px;
    border-left-color: rgba(0, 86, 172, 0.78);
    border-right-color: rgba(91, 125, 172, 0.78);
    border-top-color: rgba(0, 63, 172, 0.78);
    border-bottom-color: rgba(0, 112, 172, 0.78);
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom-left-radius: 10px;
    background-color: rgba(41, 53, 63, 0.39);
    flex-direction: column;
}

.headerBackground {
    background-color: rgb(53, 84, 130);
    height: -1%;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    border-bottom-right-radius: 8px;
    border-bottom-left-radius: 8px;
}

